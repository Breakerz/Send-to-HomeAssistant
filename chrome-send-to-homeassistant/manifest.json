{
  "name": "Send to HomeAssistant",
  "manifest_version": 2,
  "version": "0.1",
  "icons": {
    "256": "icons/icon.png"
  },
  "description": "Send current page to Home Assistant.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Send!",
    "default_icon": "icons/icon.png"
  },
  "permissions": [
    "*://*/*",
    "storage",
    "tabs"
  ],
  "options_page": "options.html"
}